{
  "$schema": "http://json.schemastore.org/template",
  "author": "Muonroi",
  "classifications": [ "Web", "Solution", "ASP.NET", "Muonroi", "Modular" ],
  "identity": "Muonroi.Modular.Solution.CSharp",
  "name": "Muonroi Modular Solution",
  "shortName": "mr-mod-sln",
  "tags": { "language": "C#", "type": "solution" },
  "preferNameDirectory": true,
  "sourceName": "Muonroi.Modular",
  "sources": [
    {
      "modifiers": [
        {
          "exclude": [
            ".git/**",
            ".github/**",
            "images/**",
            "Properties/**",
            "GlobalUsings.cs",
            "Muonroi.Modular.csproj",
            "Muonroi.Modular.Template.nuspec",
            "Muonroi.snk",
            "README_NEW.md"
          ]
        }
      ]
    }
  ],
  "primaryOutputs": [
    {
      "path": "Muonroi.Modular.sln"
    }
  ],
  "symbols": {
    "ClassName": {
      "type": "parameter",
      "datatype": "text",
      "shortName": "c",
      "replaces": "Modular",
      "fileRename": "Modular",
      "displayName": "Base Name for Classes",
      "description": "name of class: DbContext, Service, etc.",
      "defaultValue": "Modular"
    }
  }
}

